      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/containers",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7279
        }
      ],
      "UpstreamPathTemplate": "/api/containers",
      "UpstreamHttpMethod": [ "POST", "PUT" ]
    }
  ]
