<NavLink href="@Route" 
         class="@Classes">
    <FmTypography Text="@Text" 
                  Icon="@Icon"  />
</NavLink>

@code {
    [Parameter]
    public string Route { get; set; }

    [Parameter]
    public string Classes { get; set; }

    [Parameter]
    public string Text { get; set; }

    [Parameter]
    public string Icon { get; set; }
}
